{
  "name": "puml2json",
  "version": "0.0.1",
  "description": "PlantUML to json generator",
  "main": "index.js",
  "bin": {
    "puml2json": "./bin/puml2json"
  },
  "scripts": {
    "cli": "node src/cli.js",
    "test": "MOCHA_FILE=junit/test-results.xml nyc cover -- _mocha --recursive -R mocha-circleci-reporter",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "install": "node node_modules/pegjs/bin/pegjs src/parser/plantuml.pegjs",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robbito/puml2json.git"
  },
  "keywords": [
    "plantuml",
    "puml",
    "code",
    "pegjs",
    "generator",
    "uml",
    "class",
    "diagram"
  ],
  "author": "Peter Kullmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robbito/puml2json/issues"
  },
  "homepage": "https://github.com/robbito/puml2json#readme",
  "dependencies": {
    "bluebird": "^3.7.2",
    "camelcase": "^6.2.0",
    "commander": "^7.0.0",
    "debug": "^4.3.1",
    "handlebars": "^4.7.6",
    "lodash": "^4.17.20",
    "pegjs": "^0.10.0",
    "snyk": "^1.446.0"
  },
  "devDependencies": {
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-chai-expect": "^2.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^8.0.0",
    "nyc": "^15.1.0",
    "mocha-circleci-reporter": "0.0.3",
    "sinon": "^9.2.4"
  },
  "snyk": true
}
